{
  "manifest_version": 2,
  "name": "Fast Links App",
  "description": "Manage useful links",
  "version": "0.1",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": {
    "16": "images/resi-fast-links-retina-16.png",
    "128": "images/resi-fast-links-retina-128.png"
  },
  "permissions": [
    "storage",
    {
      "fileSystem": [
        "write",
        "retainEntries",
        "directory"
      ]
    },
    "https://ajax.googleapis.com/",
    "https://raw.githubusercontent.com/freewind/wind-links-scala-js-react/master/src/main/resources/data/projects.json"
  ]
}
